    {
      "Sid": "DenyEC2CreationSCP1",
      "Effect": "Deny",
      "Action": [
        "sqs:CreateQueue",
        "athena:CreateWorkGroup",
        "backup:CreateBackupVault",
        "acm:ImportCertificate",
        "acm:RequestCertificate",
        "acm-pca:CreateCertificateAuthority",
        "directconnect:CreateConnection",
        "directconnect:CreateDirectConnectGateway",
        "directconnect:CreateLag",
        "directconnect:CreatePrivateVirtualInterface",
        "directconnect:CreatePublicVirtualInterface",
        "directconnect:CreateTransitVirtualInterface",
        "ec2:CreateVolume",
        "ecr:CreateRepository",
        "ecr-public:CreateRepository",
        "eks:CreateCluster",
        "eks:CreateNodegroup",
        "elasticfilesystem:CreateFileSystem",
        "elasticfilesystem:CreateMountTarget",
        "elasticloadbalancing:CreateLoadBalancer",
        "kms:CreateKey",
        "lambda:CreateFunction",
        "mq:CreateBroker",
        "mq:CreateUser",
        "network-firewall:CreateFirewall",
        "rds:CreateDBCluster",
        "sns:CreateTopic",
        "ssm:CreateDocument",
        "transfer:CreateServer",
        "transfer:CreateUser"
      ],
      "Resource": [
        "arn:aws:ec2:*:*:instance/*",
        "arn:aws:sqs:*:*:*",
        "arn:aws:athena:*:*:workgroup/*",
        "arn:aws:backup:*:*:backup-vault:*",
        "arn:aws:acm:*:*:certificate/*",
        "arn:aws:acm-pca:*:*:certificate-authority/*",
        "arn:aws:directconnect:*:*:dxlag/*",
        "arn:aws:ec2:*:*:volume/*",
        "arn:aws:ecr:*:*:repository/*",
        "arn:aws:ecr-public::*:repository/*",
        "arn:aws:eks:*:*:cluster/*",
        "arn:aws:elasticfilesystem:*:*:file-system/*",
        "arn:aws:elasticloadbalancing:*:*:loadbalancer/*",
        "arn:aws:kms:*:*:key/*",
        "arn:aws:lambda:*:*:function:*",
        "arn:aws:mq:*:*:broker:*:*",
        "arn:aws:network-firewall:*:*:firewall/*",
        "arn:aws:rds:*:*:cluster:*",
        "arn:aws:sns:*:*:*",
        "arn:aws:ssm:*:*:document/*",
        "arn:aws:transfer:*:*:server/*"
      ],
      "Condition": {
        "Null": {
          "aws:RequestTag/map-migrated": "true"
        }
      }
    }